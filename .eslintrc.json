  
{
    "extends": "airbnb-base",
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "error",
            "info"
          ]
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "next"
        }
      ]
    }
  }